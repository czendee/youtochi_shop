<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/ic_landscape" >


    <LinearLayout android:orientation="vertical"

        tools:context="com.tochi.nuevaetapabolas.Fragments.CuartoFragment"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:android="http://schemas.android.com/apk/res/android">

        <Button
            android:id="@+id/botonRegresarLista"
            android:layout_width="100dp"
            android:layout_height="100dp"

            android:background="@mipmap/ic_botonflechaleft"
            android:focusable="true"
            android:textColor="#000000"
            android:text="-Regresar"
            android:textStyle="bold"

            />

        <TextView
            android:id="@+id/textResult"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/vacio"
            android:textSize="10sp" />


        <TextView
            android:id="@+id/textTituloLista"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/tituloLista"
            android:textSize="10sp" />

        <ListView android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:id="@+id/lstTasks"
            />

    </LinearLayout>



</RelativeLayout>
